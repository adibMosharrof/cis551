<style>

</style>

<% include partials/header.ejs %>
<div class="container">
  <div class="card container-card">
    <h1 class="card-title">Select a housing complex</h1>
    <div class="row card-deck">

        <% if (housing_complexes.length > 0) {%>
            <% housing_complexes.forEach((housing_complex, index) => { %>
              <div class="col-sm card  housing_complex" onclick="myGo(<%= housing_complex.id%>)">
                  <div class="card-body">
                    <h3 class="card-title card-link"><%= housing_complex.name %></h3>
                    <div>
                      <img src="<%= housing_complex.name%>.jpg" alt="">
                    </div>
                  </div>
              </div>
            <% }) %>
        <% } else { %>
            <p class="text-center">No housing complexes found</p>
        <% } %>
    </div>
    </div>
</div>
<script type="text/javascript">
  function myGo(id){
    window.location = '/housing_complex/'+id;
  }
</script>
<% include partials/footer.ejs %>
